<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zbl.code.system.dao.ConfigMapper">

    <select id="list" resultType="com.zbl.code.system.pojo.vo.ConfigVO">
        select sc.* from sys_config as sc
        ORDER BY sc.create_time DESC
    </select>
</mapper>
